<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Tsukuru"
                    xmlns:sourcePawnViewModels="clr-namespace:Tsukuru.SourcePawn.ViewModels"
                    xmlns:sourcePawnViews="clr-namespace:Tsukuru.SourcePawn.Views">
    <DataTemplate DataType="{x:Type sourcePawnViewModels:SourcePawnCompileViewModel}">
        <sourcePawnViews:SourcePawnCompilerView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type sourcePawnViewModels:SettingsViewModel}">
        <sourcePawnViews:SettingsView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type sourcePawnViewModels:PostBuildActionsViewModel}">
        <sourcePawnViews:PostBuildActionsView />
    </DataTemplate>
</ResourceDictionary>